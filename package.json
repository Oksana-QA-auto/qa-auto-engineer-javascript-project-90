{
  "name": "@hexlet/code",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "serve:e2e": "npm run build && npm run preview -- --port=5173",
    "playwright:install": "npx playwright install",
    "playwright:install:deps": "npx playwright install --with-deps",
    "pretest:e2e": "npx playwright install --with-deps",
    "test:e2e": "cross-env DND_ENABLED=false FILTER_ENABLED=false npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:step1": "npx playwright test tests/step1-basic.spec.js",
    "test:e2e:step2": "npx playwright test tests/step2-auth.spec.js",
    "test:e2e:step3": "npx playwright test tests/step3-users.spec.js",
    "test:e2e:report": "npx playwright show-report"
  },
  "dependencies": {
    "@hexlet/testing-task-manager": "^0.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "1.54.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "playwright": "1.54.0",
    "vite": "^4.1.3"
  }
}
